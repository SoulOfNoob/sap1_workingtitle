{
  "name": "SAP Google Location Connector",
  "version": "1.0",
  "description": "The ultimate plugin!",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.3.1.min.js"]
    }
  ],
  "background": {
    "scripts": [ "background.js"],
    "persistent": true
  },
  "permissions": [ "storage", "tabs", "activeTab", "webNavigation", "*://*/*" ],
  "manifest_version": 2
}